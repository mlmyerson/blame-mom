{
  "name": "blame-mom",
  "version": "1.0.0",
  "description": "A satirical site that transforms real news headlines to humorously blame 'your mother' for world events",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client:dev\"",
    "test": "jest",
    "client:dev": "npm run dev --prefix app",
    "client:build": "npm run build --prefix app",
    "client:preview": "npm run preview --prefix app",
    "build": "npm run client:build"
  },
  "keywords": [
    "satire",
    "news",
    "humor",
    "headlines"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "jest": "^30.2.0"
  }
}
